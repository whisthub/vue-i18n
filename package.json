{
  "name": "@whisthub/vue-i18n",
  "version": "0.0.0",
  "private": false,
  "type": "module",
  "description": "Simple internationalization plugin for Vue, modeled after vue-i18n, but usable with both the options api and composition api in the same project.",
  "main": "lib/index.js",
  "exports": {
    ".": "./lib/index.js",
    "./plugin-vite": "./lib/plugins/vite-plugin.js"
  },
  "imports": {
    "#lib/*": "./lib/*",
    "#test/*": "./test/*"
  },
  "scripts": {
    "lint": "eslint",
    "test": "mocha test/**/*-test.js --config test/.mocharc.cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/whisthub/vue-i18n.git"
  },
  "keywords": [
    "vue",
    "i18n",
    "options",
    "composition"
  ],
  "author": "Sebastiaan Marynissen <sebastiaan@whisthub.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/whisthub/vue-i18n/issues"
  },
  "homepage": "https://github.com/whisthub/vue-i18n#readme",
  "devDependencies": {
    "@vue/test-utils": "^2.4.6",
    "@whisthub/eslint-config": "^3.1.0",
    "chai": "^6.2.0",
    "eslint": "^9.39.1",
    "jsdom": "^27.1.0",
    "mocha": "^11.7.5",
    "vue": "^3.5.24",
    "vue-esm-loader": "^1.0.0"
  },
  "dependencies": {
    "@intlify/message-compiler": "^11.1.12"
  }
}
